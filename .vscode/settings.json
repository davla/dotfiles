{
  "black-formatter.importStrategy": "fromEnvironment",
  "files.associations": {
    "*.list": "ini",
    "*.rc": "ini",
    "*.theme": "ini",
    "*hidpi": "jinja",
    "*lodpi": "jinja",
    "dunstrc": "ini",
    "fonts.conf": "xml",
    "gtkrc": "ini",
    "hgrc": "ini",
    "mrconfig*": "ini",
    "picom.conf": "toml",
    "xfce-applications.menu": "xml",
    "**/.i3/**/*": "i3",
    "**/.sway/**/*": "i3",
    "**/apt/**/*.list": "debsources",
    "**/git/ignore": "ignore",
    "**/geany/filedefs/**": "ini",
    "**/i3-sway/**/*": "i3",
    "**/i3/**/*": "i3",
    "**/i3blocks*/**/*": "ini",
    "**/mako/config": "ini",
    "**/mr/**/*": "shellscript",
    "**/pacman.d/**/*": "ini",
    "**/sway/**/*": "i3",
  },
  "isort.args": [
    "--profile",
    "black"
  ],
  "isort.importStrategy": "fromEnvironment",
  "python.analysis.autoImportCompletions": true,
  "python.analysis.extraPaths": [
    "./dotfiles/dotfiles/usr/local/lib/i3ipc-extras/src"
  ],
  "python.envFile": "${workspaceFolder}/.dotfiles-env",
  "[python]": {
    "editor.codeActionsOnSave": {
      "source.organizeImports": "explicit"
    },
    "editor.defaultFormatter": "ms-python.black-formatter",
    "editor.formatOnSave": true,
    "editor.rulers": [
      88
    ]
  },
}
